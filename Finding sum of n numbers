#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n,c,sum=0,value;
    scanf("%d",&n);
    for(c=1;c<=n;c++)
    {
        sum+=c;
        
    }
    printf("%d",sum);
    return 0;
}
