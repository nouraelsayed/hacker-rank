#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
int ch;
int a,b ,c;
scanf("%d",&ch);
scanf("%d,%d",&a,&b);
 switch(ch)
    {
        case 1: 
            c =a + b;
            break;

        case 2: 
            c =a - b;
            break;

        case 3: 
             c =a * b;
            break;

        case 4: 
             c =a / b;
            break;

     
    }
    printf("%d",c);
    return 0;
}
